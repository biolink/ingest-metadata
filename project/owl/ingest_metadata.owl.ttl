            owl:onProperty biolink:target_format ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty biolink:orphan_node_count ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty biolink:target_creation_date ],
