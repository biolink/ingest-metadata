        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty biolink:ingest_code_url ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty biolink:file_name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty biolink:target_model_url ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty biolink:node_normalizer ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty biolink:node_normalizer_version ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty biolink:file_name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty biolink:ingest_code_url ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty biolink:total_edge_count ],
        [ a owl:Restriction ;
