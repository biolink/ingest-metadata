            owl:onProperty biolink:source_access_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Date ;
            owl:onProperty biolink:source_access_date ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty biolink:file_creation_date ],
